{
	"name": "og.re-taro.dev",
	"type": "module",
	"version": "3.0.6",
	"packageManager": "pnpm@9.7.1",
	"description": "My personal OG API",
	"author": "Rintaro Itokawa <me@re-taro.dev> (https://re-taro.dev)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/re-taro/og.re-taro.dev"
	},
	"scripts": {
		"build": "wrangler deploy --dry-run --minify --outdir dist",
		"fmt": "run-s -c fmt:*",
		"fmt:eslint": "pnpm run lint:eslint --fix",
		"lint": "run-p -c lint:*",
		"lint:eslint": "eslint .",
		"lint:tsc": "tsc -p . --noEmit",
		"start": "wrangler dev --remote"
	},
	"dependencies": {
		"@resvg/resvg-wasm": "2.6.2",
		"budoux": "0.6.2",
		"hono": "4.5.7",
		"satori": "0.10.14",
		"yoga-wasm-web": "0.3.3"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "4.20240821.1",
		"@re-taro/eslint-config": "4.1.0",
		"@types/react": "18.3.4",
		"eslint": "9.9.0",
		"npm-run-all2": "6.2.2",
		"react": "18.3.1",
		"typescript": "5.5.4",
		"wrangler": "3.72.1"
	}
}
