{
	"name": "og.re-taro.dev",
	"version": "3.0.0",
	"packageManager": "pnpm@8.15.4",
	"author": "Rintaro Itokawa <me@re-taro.dev> (https://re-taro.dev)",
	"type": "module",
	"description": "My personal OG API",
	"repository": {
		"type": "git",
		"url": "https://github.com/re-taro/og.re-taro.dev"
	},
	"license": "MIT",
	"scripts": {
		"build": "wrangler deploy --dry-run --minify --outdir dist",
		"fmt": "run-s -c fmt:*",
		"fmt:eslint": "pnpm run lint:eslint --fix",
		"fmt:prettier": "prettier . -w",
		"lint": "run-p -c lint:*",
		"lint:eslint": "eslint .",
		"lint:prettier": "prettier . -c",
		"lint:tsc": "tsc -p . --noEmit",
		"start": "wrangler dev --remote"
	},
	"dependencies": {
		"@resvg/resvg-wasm": "2.4.1",
		"budoux": "0.6.2",
		"hono": "4.0.9",
		"satori": "0.10.13",
		"yoga-wasm-web": "0.3.3"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "4.20240222.0",
		"@re-taro/eslint-config": "3.1.1",
		"@re-taro/prettier-config": "2.0.0",
		"@types/react": "18.2.61",
		"eslint": "8.57.0",
		"npm-run-all2": "6.1.2",
		"prettier": "3.2.5",
		"react": "18.2.0",
		"typescript": "5.3.3",
		"wrangler": "3.30.1"
	}
}
